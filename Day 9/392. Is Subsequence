class Solution {
    public boolean isSubsequence(String s, String t) 
    {
        int sl = s.length();
        int tl = t.length();
        
        if(tl==0 && sl==0)
            return true;
        
        if(tl==0)   //for sure sl is not 0
            return false;
        
        if(sl==0)   //for sure tl is not 0
            return true;
        
        int j=0;
        for(int i=0;i<tl && j<sl;i++)
        {
            if(s.charAt(j)==t.charAt(i))
            {
               j++;
               //break;
            }
                
        }
        if(j==sl)
            return true;
        return false;
    }
}
